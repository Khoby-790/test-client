{
  "name": "testfrontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "codegen:schema": "npx apollo client:download-schema src/graphql/schema.json --endpoint=http://localhost:4000/graphql",
    "codegen:generate": "npx apollo client:codegen --localSchemaFile=src/graphql/schema.json --globalTypesFile=src/graphql/__generated__/globalTypes.ts --includes=src/graphql/**/*.ts --target=typescript",
    "cd": "yarn codegen:schema && yarn codegen:generate"
  },
  "dependencies": {
    "@apollo/client": "^3.7.5",
    "@emotion/react": "^11.10.5",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.14",
    "@mantine/core": "^5.10.2",
    "@mantine/hooks": "^5.10.2",
    "@mantine/notifications": "^5.10.2",
    "@reduxjs/toolkit": "^1.9.1",
    "@superstate/core": "^0.1.0",
    "@tabler/icons": "^2.1.2",
    "@types/node": "^18.11.18",
    "apollo": "^2.34.0",
    "clsx": "^1.2.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "random-image-unsplash": "^1.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-hook-inview": "^4.5.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
